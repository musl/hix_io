
<div class="post">
	<p>
	<label for="post-search">Search:</label>
	<input type="text" id="post-search" can-enter="search" value="{{q}}">
	</p>
</div>

<div class="post">
	{{#if posts}}
		<p>poop</p>
		<ul>
		{{#posts}}
			<li><a href="#!posts/{{id}}">{{title}}</a></li>
		{{/posts}}
		</ul>
	{{else}}
		<p><i class="fa fa-large fa-exclamation-triangle"></i>No results.</p>	
	{{/if}}
</div>
